<nav class="navbar navbar-main navbar-expand-lg  position-sticky mt-4 top-1 px-0 mx-4 shadow-none border-radius-xl z-index-sticky" id="navbarBlur" data-scroll="true">
  <div class="w-100 d-flex justify-content-between align-items-center">
    <div class="container-fluid py-1 px-3" onclick="controlCollalpseMenu()">
      <div class="sidenav-toggler sidenav-toggler-inner d-xl-block d-none ">
        <a href="javascript:;" class="nav-link text-body p-0">
          <div class="sidenav-toggler-inner">
            <i class="sidenav-toggler-line"></i>
            <i class="sidenav-toggler-line"></i>
            <i class="sidenav-toggler-line"></i>
          </div>
        </a>
      </div>
    </div>

    <div class="w-50 d-flex justify-content-end align-items-center gap-2 me-3">
      <div style="text-align: right;" class="d-flex flex-column">
        <label class="m-0"  id="userNameHeader"> <%- userData.nombre %> </label>
        <label for="" class="m-0 closeSession" style="font-size: 11px; cursor: pointer;">
          <a href="/login/logout">
            Cerrar Sesion
          </a>
        </label>
      </div>
      <div style="width: 50px; height: 50px; border-radius: 50%; background-color: lightgray;">
        <img src="http://104.236.159.193:3010/<%-userData.foto%>" class="imgClass" alt="" >
      </div>
    </div>

  </div>
</nav>  


<script>
  const controlCollalpseMenu = () => {
    if ($("body").hasClass("g-sidenav-hidden")) {
      $("#collapseCatalog").collapse("hide")
      $("#collapseReport").collapse("hide")
      $("#collapseActions").collapse("hide")
      $("#collapseConfig").collapse("hide")
    } else {
      $("#collapseCatalog").collapse("show")
      $("#collapseReport").collapse("show")
      $("#collapseActions").collapse("show")
      $("#collapseConfig").collapse("show")
    }
  }
</script>


